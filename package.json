{
  "name": "randomwallpaper-windows",
  "version": "0.0.1",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build:renderer && tsc",
    "start": "npm run build && electron .",
    "dev": "concurrently \"vite\" \"tsc -w\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:renderer": "vite",
    "build:renderer": "vite build"
  },
  "devDependencies": {
    "electron": "^39.2.3",
    "typescript": "^5.0.0",
    "@types/node-fetch": "^2.6.2",
    "vite": "^7.2.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^8.0.0",
    "wait-on": "^7.0.0"
  },
  "dependencies": {
    "vue": "^3.5.24",
    "element-plus": "^2.11.7",
    "node-fetch": "^2.6.7",
    "wallpaper": "^7.0.0"
  }
}
